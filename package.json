{
  "name": "sv2020",
  "version": "1.0.0",
  "description": "best web for best ph",
  "author": "iasvobodin",
  "scripts": {
    "export": "nuxt export",
    "serve": "nuxt serve",
    "static": "nuxt build --target static && nuxt export --fail-on-error",
    "analyze": "cross-env NODE_ENV=production  nuxt build --analyze",
    "buildandstart": "cross-env NODE_ENV=production nuxt build && cross-env NODE_ENV=production nuxt start",
    "exif": "exiftool  -json -filename -ShutterSpeedValue -ApertureValue -FocalLength -ImageWidth -ImageHeight -Rating -Keywords 'src/image'> 'src/db/data.json'",
    "createdb": "node src/db/Createdb.js",
    "sqip": "node src/db/sqip.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "deploy": "npm run build && sls deploy",
    "start-sls": "npm run build && sls offline start",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/style-resources": "1.0.0",
    "animejs": "^3.2.0",
    "curtainsjs": "^6.1.1",
    "dotenv": "^8.2.0",
    "glslify-loader": "^2.0.0",
    "gsap": "file:gsap-bonus.tgz",
    "justified-layout": "^4.0.0",
    "normalize.css": "8.0.1",
    "nuxt-start": "^2.13.2",
    "raw-loader": "^4.0.1",
    "serverless-http": "^2.5.0",
    "splitting": "^1.0.6",
    "vue-observe-visibility": "^0.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "browser-env": "^3.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "fastify": "^2.14.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "nuxt": "^2.13.2",
    "nuxt-compress": "^2.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "serverless-offline": "^6.3.0",
    "serverless-s3-sync": "^1.14.3"
  }
